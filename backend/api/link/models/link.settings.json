{
  "kind": "collectionType",
  "collectionName": "links",
  "info": {
    "name": "link"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true
    },
    "aliveMs": {
      "type": "integer",
      "max": 180000
    },
    "categories": {
      "collection": "category",
      "via": "links"
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "links",
      "dominant": true
    }
  }
}
